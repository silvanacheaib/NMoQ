<action name="actioner" type="Javascript" autorun="onstart"><![CDATA[
    for (let i = krpano.hotspot.count - 1; i >= 0; i--) {
        const hotspot = krpano.hotspot.getItem(i);
 
        if (hotspot.linkedscene && hotspot.style !== "infoHotspot") {
            const previewName = "preview_" + hotspot.name;
            const scene = krpano.scene.getItem(hotspot.linkedscene);
            const thumbUrl = scene ? (scene.thumburl || `panos/${hotspot.linkedscene}.tiles/thumb.jpg`) :
                                     `panos/${hotspot.linkedscene}.tiles/thumb.jpg`;
 
            if (!krpano.get("hotspot[" + previewName + "]")) {
                // Add thumbnail preview hotspot
                krpano.call("addhotspot(" + previewName + ")");
                // Position adjustments:
                // - Moved 5° right (ath+5)
                // - Positioned above hotspot (atv-5 instead of below)
                krpano.set("hotspot[" + previewName + "].ath", hotspot.ath + 1);
                krpano.set("hotspot[" + previewName + "].atv", hotspot.atv + 1);
                krpano.set("hotspot[" + previewName + "].url", thumbUrl);
                krpano.set("hotspot[" + previewName + "].scale", 0.5);
                krpano.set("hotspot[" + previewName + "].alpha", 0);
				krpano.set("hotspot[" + previewName + "].flying", 0);
                krpano.set("hotspot[" + previewName + "].visible", true);
                krpano.set("hotspot[" + previewName + "].enabled", true);
                krpano.set("hotspot[" + previewName + "].zorder", 9999);
                krpano.set("hotspot[" + previewName + "].renderer", "css3d");
                krpano.set("hotspot[" + previewName + "].distorted", false);
                krpano.set("hotspot[" + previewName + "].edge", "centerbottom");
                krpano.set("hotspot[" + previewName + "].origin", "center");
                krpano.set("hotspot[" + previewName + "].onloaded",
                    "js: caller.sprite.classList.add('thumbnail-preview'); " +
                    "set(caller.visible,true); tween(alpha,1,0.3);");
 
                // Hover behavior: show preview only
                krpano.set("hotspot[" + hotspot.name + "].onhover",
                    "set(hotspot[" + previewName + "].visible,true); " +
                    "tween(hotspot[" + previewName + "].alpha,1,0.2);");
 
                // Mouse out behavior: hide preview
                krpano.set("hotspot[" + hotspot.name + "].onout",
                    "tween(hotspot[" + previewName + "].alpha,0,0.3,default," +
                    "set(hotspot[" + previewName + "].visible,false));");
 
                // Keep preview visible when hovering over the thumbnail itself
                krpano.set("hotspot[" + previewName + "].onover",
                    "set(visible,true);");
                krpano.set("hotspot[" + previewName + "].ondown",
                    "set(visible,true);");
            }
        }
    }
]]></action>
 
 
<events onnewscene="rebuild_thumbnails();" />
 
<action name="rebuild_thumbnails" type="Javascript"><![CDATA[
    // Remove all existing preview hotspots first
    for(let i = krpano.hotspot.count - 1; i >= 0; i--) {
        const hotspot = krpano.hotspot.getItem(i);
        if(hotspot.name.startsWith("preview_")) {
            krpano.call("removehotspot(" + hotspot.name + ")");
        }
    }
    // Rebuild thumbnails
    krpano.call("actioner();");
]]>
